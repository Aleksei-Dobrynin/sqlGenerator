{
  "LLMParser": {
    "Enabled": true,
    "ApiEndpoint": "http://localhost:11434/v1/chat/completions",
    "Model": "Qwen2.5-coder-7b-instruct",
    "Temperature": 0.1,
    "MaxTokens": 32768,
    "TimeoutSeconds": 600,
    "RetryCount": 2,
    "RetryDelayMs": 2000,
    "SystemPrompt": "You are a precise SQL parser that converts PostgreSQL CREATE TABLE statements to JSON. Your output must be ONLY valid JSON array without any markdown formatting, code blocks, or explanations. ALWAYS return array format: [{}] even for single table.",
    "ResponseFormat": "json_object",
    "LogRequests": true,
    "CacheResponses": true,
    //"EnableBatching": true,
    //"MaxTablesPerBatch": 10,
    "MaxScriptSizeForSingleRequest": 50000,
    //"StreamingEnabled": false,
    "ContextWindow": 32768,
    "ReservedTokensForResponse": 16384
  },
  "Parser": {
    "DefaultMode": "Standard",
    //"DefaultMode": "LLM", 
    "ShowProgressIndicator": true,
    "AutoSelectParserBySize": true,
    "LargeScriptThreshold": 10000
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "SQLFileGenerator": "Debug"
    },
    "LogFile": "logs/parser_{date}.log",
    "EnableFileLogging": true
  }
}